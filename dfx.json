{
  "canisters": {
    "buoy-ai-backend": {
      "type": "custom",
      "build": "npx azle build buoy-ai-backend",
      "candid": "src/buoy-ai-backend/buoy-ai-backend.did",
      "wasm": ".azle/buoy-ai-backend/buoy-ai-backend.wasm",
      "gzip": true,
      "main": "src/buoy-ai-backend/src/index.ts",
      "tech_stack": {
        "cdk": { "azle": {} },
        "language": { "javascript": {}, "typescript": {} }
      }
    },
    "buoy-ai-frontend": {
      "dependencies": [
        "buoy-ai-backend"
      ],
      "source": [
        "src/buoy-ai-frontend/assets"
      ],
      "type": "assets"
    },
    "scraper": {
      "type": "rust",
      "package": "scraper",
      "candid": "src/scraper/src/scraper.did",
      "main": "src/scraper/src/lib.rs"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
